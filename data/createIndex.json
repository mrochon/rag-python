                    "prioritizedContentFields": [
                        {
                        "fieldName": "content"
                        }
                    ],
                    "prioritizedKeywordsFields": [
                    ]
                }
            }
        ]
    },
    "vectorSearch": {
        "profiles": [
        {
            "name": "myHnswProfile",
            "algorithm": "myHnsw"
        },
        {
            "name": "myAlgorithm",
            "algorithm": "myExhaustive"
        }
        ],
        "algorithms": [
        {
            "name": "myHnsw",
            "kind": "hnsw",
            "hnswParameters": {
                "m": 4,
                "metric": "cosine"
            }
        },
        {
            "name": "myExhaustive",
            "kind": "exhaustiveKnn",
            "exhaustiveKnnParameters": {
                "metric": "cosine"
            }
        }
