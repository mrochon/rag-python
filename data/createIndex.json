            "searchable": true,            
            "filterable": false,
            "sortable": false,
            "facetable": false
          },
        {
            "name": "contentEmbedding",
            "type": "Collection(Edm.Single)",
            "searchable": true,
            "filterable": false,
            "retrievable": false,
            "synonymMaps": [],
            "dimensions": 1536,
            "vectorSearchProfile": "myHnswProfile"
